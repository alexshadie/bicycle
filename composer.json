    "type": "library",
    "require": {
        "php": "^7.1",
        "psr/log": "^1.0",
        "psr/container": "^1.0",
        "monolog/monolog": "^1.17",
        "mikecao/flight": "^1.3",
        "symfony/dependency-injection": "4.0.4",
        "symfony/config": "4.0,4",
        "symfony/yaml": "4.0.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "phpunit/php-code-coverage": "^6.0",
        "phpunit/dbunit": "^4.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Alex Shadie",
            "email": "alex@astra.ws"
        }
    ],
    "autoload" : {
